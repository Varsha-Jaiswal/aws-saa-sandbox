{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "S3 Bucket CloudFormation Template",
    "Parameters": {
        "BucketName": {
            "Description": "S3 Bucket Name",
            "Type": "String",
            "Default": "s3-sandbox-vj-cfn"
        }
    },
    "Resources": {
        "S3Bucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {
                "BucketName": {
                    "Ref": "BucketName"
                }
            }
        }
    }
}